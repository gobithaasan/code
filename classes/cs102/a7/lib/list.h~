#ifndef LIB_LIST_H_
#define LIB_LIST_H_

#define EMPTY_LIST NULL

typedef struct list_s {
  void * head;
  struct list_s * next;
} list_t;

list_t * list_cons(list_t * list, void * head);
void     list_dispose(list_t ** list);
list_t * list_init();

#endif  // LIB_LIST_H_
